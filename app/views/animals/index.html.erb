<div class="jumbotron">
  <h1>Baby Animals!</h1>
  <p>For the tough times in life</p>
  <p><a class="btn btn-primary btn-lg" role="button" href="<%=new_animal_path %>"> New Picture</a></p>
</div>
<div class="container">
  <div class="row">
  <div class="col-md-6">
<%@animals.each do |animal| %>
<div class="panel panel-default">
  <font size="6"><%=animal.title %></font>
  <div class="panel body">
    <a href="animals/<%=animal.id%>"> <IMG HEIGHT="400" WIDTH="450" SRC="<%=animal.image_url %>"></a>
  </div>
  <font size="3"><%=animal.tags%></font>
    <% end %>
<br>
